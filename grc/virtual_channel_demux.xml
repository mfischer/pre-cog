<?xml version="1.0"?>
<!--
###################################################
##Virtual Channel Demux
###################################################
 -->
<block>
    <name>Virtual Channel Demux</name>
    <key>virtual_channel_demux</key>
    <import>import precog</import>
    <make>precog.virtual_channel_demux($port_count)</make>
    <param>
        <name>Output Ports</name>
        <key>port_count</key>
        <value>2</value>
        <type>int</type>
    </param>
    <sink>
        <name>in</name>
        <type></type>
        <optional>1</optional>
    </sink>
    <source>
        <name>out</name>
        <type></type>
        <nports>$port_count</nports>
        <optional>1</optional>
    </source>
 
    <doc>
Virtual channel demux.
    </doc>
</block>
